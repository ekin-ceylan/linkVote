{
    "printWidth": 250,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "pugAttributeSeparator": "none",
    "pugSingleQuote": false,
    "pugSortAttributesBeginning": ["^id$", "^name$", "^type$", "v-model", "^value$"],
    "pugSortAttributesEnd": ["validate", "^data-vv", "class"],
    "pugPrintWidth": 500,
    "pugWrapAttributesThreshold": 12,
    "pugFramework": "vue",
    "pugIdNotation": "as-is"
}
